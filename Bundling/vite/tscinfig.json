{
  "compilerOptions": {
    "esModuleInterop": true,
    "isolatedModules": true,
    "module": "esnext",
    "moduleResolution": "bundler",
    "lib": ["esnext", "dom"],
    "noEmit": true,
    "skipLibCheck": true,
    "noImplicitAny": false,
    "resolveJsonModule": true,
    "sourceMap": true,
    "target": "esnext",
    "useDefineForClassFields": true
  },
  "include": ["src"]
}
